% introduce options
\DeclareOptionBeamer{compress}{\beamer@compresstrue}

\newif\ifbeamer@framenumber
\beamer@framenumberfalse
\DeclareOptionBeamer{framenumber}{\beamer@framenumbertrue}

\newif\ifbeamer@totalframenumber
\beamer@totalframenumberfalse
\DeclareOptionBeamer{totalframenumber}{\beamer@totalframenumbertrue}

\newif\ifbeamer@contents
\beamer@contentsfalse
\DeclareOptionBeamer{contents}{\beamer@contentstrue}

\newif\ifbeamer@navigation
\beamer@navigationfalse
\DeclareOptionBeamer{navigation}{\beamer@contentstrue}

\newif\ifbeamer@NL
\beamer@NLfalse
\DeclareOptionBeamer{NL}{\beamer@NLtrue}

\newif\ifbeamer@FWO
\beamer@FWOfalse
\DeclareOptionBeamer{FWO}{\beamer@FWOtrue}

\newif\ifbeamer@EMAT
\beamer@EMATfalse
\DeclareOptionBeamer{EMAT}{\beamer@EMATtrue}

\newif\ifbeamer@CMT
\beamer@CMTfalse
\DeclareOptionBeamer{CMT}{\beamer@CMTtrue}

\newif\ifbeamer@PPG
\beamer@PPGfalse
\DeclareOptionBeamer{PPG}{\beamer@PPGtrue}

\ProcessOptionsBeamer
% -------------------------------------------------------------------------

\mode<presentation>

\useinnertheme{UniversiteitAntwerpen}
\usefonttheme {UniversiteitAntwerpen}
\usecolortheme{UniversiteitAntwerpen}
\useoutertheme{UniversiteitAntwerpen}

\mode
<all>
